<%= form_for @import do |f| %>
  <% if @import.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@type.errors.count, "error") %> prohibited this type from being saved:</h2>

      <ul>
      <% @import.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, :size => "80" %>
  </div>
  <div class="field">
    <%= f.label :url %><br />
    <%= f.text_field :url, :size => "80" %>
  </div>
  <div class="field">
    <%= f.label :comments %><br />
    <%= f.text_area :comments, :size => "80x10" %><br />
  </div>
  <div class="field">
    <%= f.label :license %><br />
    <%= f.text_area :license, :size => "80x10" %><br />
  </div>
  <div class="field">
    <%= f.check_box :autoload %> <%= f.label :autoload %>
  </div>
  <div class="field">
    <%= f.check_box :muni %> <%= f.label :municipal %>
  </div>
  <br />
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
